{
  "name": "ThemeName",
  "version": "1.0.0",
  "main": "index.html",
  "license": "MIT",
  "config": {
    "tailwind_input": "assets/scss/tailwind.scss",
    "tailwind_output": "assets/scss/vendors/_tailwind.scss",
    "scss_file": "assets/scss/app.scss",
    "css_output": "assets/css/app.min.css",
    "js_files": "assets/js/vendors/jquery-3.6.0.min.js assets/js/main.js",
    "js_output": "assets/js/app.min.js"
  },
  "scripts": {
    "sass": "sass ${npm_package_config_scss_file} ${npm_package_config_css_output}",
    "tailwind": "npx tailwindcss -i ${npm_package_config_tailwind_input} -o ${npm_package_config_tailwind_output}",
    "js": "uglifyjs ${npm_package_config_js_files} -o ${npm_package_config_js_output}",
    "dev": "concurrently 'yarn tailwind --watch' 'yarn sass --watch'",
    "autoprefixer": "postcss assets/css/app.min.css --replace --use autoprefixer --no-map",
    "build": "yarn sass --style=compressed --no-source-map && yarn autoprefixer && rm -rf ${npm_package_config_css_output}.map && yarn js"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "uglify-js": "^3.17.4",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e"
  }
}
